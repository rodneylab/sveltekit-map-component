{
	"name": "@rodneylab/sveltekit-map-component",
	"description": "Handy components for building sites in SvelteKit",
	"version": "2.0.0",
	"author": "Rodney Lab <ask@rodneylab.com> (@rodneyLab)",
	"license": "BSD-3-Clause",
	"keywords": [
		"svelte",
		"sveltekit",
		"map",
		"mapbox",
		"openstreetmap"
	],
	"svelte": "index.js",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --fail-on-hints",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"prettier:check": "prettier --check --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@commitlint/config-conventional": "^17.3.0",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-static": "1.0.0-next.48",
		"@sveltejs/kit": "next",
		"@sveltejs/package": "1.0.0-next.6",
		"@types/leaflet": "^1.9.0",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"commitlint": "^17.3.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"husky": "^8.0.2",
		"leaflet": "^1.9.3",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"svelte": "^3.53.1",
		"svelte-check": "^2.9.2",
		"svelte-preprocess": "^4.10.7",
		"svelte2tsx": "^0.5.21",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^3.2.4"
	},
	"type": "module",
	"peerDependencies": {
		"leaflet": "^1.9.3"
	}
}
